{
  "name": "mock-gemini-ai",
  "version": "1.0.0",
  "description": "Mock Google Vertex AI Gemini API server for testing",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:unit": "vitest run --exclude '__tests__/**/integration.test.ts' --exclude '__tests__/integration/**'",
    "test:integration": "RUN_INTEGRATION_TESTS=true vitest run __tests__/**/integration.test.ts",
    "test:integration:real": "RUN_INTEGRATION_TESTS=true COMPARE_WITH_REAL_APIS=true vitest run __tests__/**/integration.test.ts",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "gemini",
    "vertex-ai",
    "mock",
    "api",
    "testing"
  ],
  "author": "Andy Savage <andy@savage.hk",
  "license": "MIT",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "@google-cloud/vertexai": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@ai-sdk/google": "^1.2.22",
    "@ai-sdk/google-vertex": "^2.2.27",
    "@google/generative-ai": "^0.24.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.14",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/coverage-v8": "^3.2.4",
    "ai": "^4.3.19",
    "eslint": "^9.31.0",
    "supertest": "^7.1.3",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "zod": "^3.25.76"
  }
}
