{
  "name": "mock-gemini-api",
  "version": "1.0.0",
  "description": "Mock Google Vertex AI Gemini API server for testing",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:run": "vitest run",
    "test:unit": "vitest run --exclude '__tests__/**/integration.test.ts'",
    "test:integration": "RUN_INTEGRATION_TESTS=true vitest run __tests__/**/integration.test.ts",
    "test:integration:real": "RUN_INTEGRATION_TESTS=true COMPARE_WITH_REAL_APIS=true vitest run __tests__/**/integration.test.ts",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "gemini",
    "vertex-ai",
    "mock",
    "api",
    "testing"
  ],
  "author": "Andy Savage <andy@savage.hk",
  "license": "MIT",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "@google-cloud/vertexai": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  }
}
